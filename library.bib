title = {{Prediction of tree diameter growth using quantile regression and mixed-effects models}},
url = {http://dx.doi.org/10.1016/j.foreco.2014.02.006 http://www.rnr.lsu.edu/people/cao/2014 Bohora and Cao For. Ecol. Mgt. 319-62-66.pdf},
